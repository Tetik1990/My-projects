@model IEnumerable<string>

<div data-role="navbar">

    <ul>

        @foreach (var link in Model)
        {
            <li   style="background-color: gray; text-align: center; list-style-type: none">
                @Html.RouteLink(link, new
                {
                    controller = "Home",
                    action = "List",
                    genre = link,
                    page = 1
                }, new
                {
                    data_transition = "fade",
                    @class = (link == ViewBag.SelectedGenre ? " ui-btn-active btn btn-success" : null)
                })
            </li>
        }

    </ul>
</div>